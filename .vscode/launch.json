    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/target/debug/stegano-ants",
            "args": [
                "--cycles=1",
                "--dispatcher=biased",
                "--updater=const",
                "--converter=superpixels:5",
                "--target-capacity=50000B",
                "embed",
                "--data",
                "assets/data/lorem_ipsum_large.txt",
                "--image",
                "assets/images/baboon/baboon-s.bmp"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb"
        }
    ]
}
